{
  "name": "eslint-config-it4c",
  "version": "0.4.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --max-warnings 0 .",
    "inspect-rules": "tsx scripts/inspect-rules.ts",
    "inspect-rules:check": "tsx scripts/inspect-rules.ts | diff rules.json -",
    "generate-rules-md": "tsx scripts/generate-rules-md.ts",
    "update-rules": "tsx scripts/inspect-rules.ts > rules.json && npm run generate-rules-md"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/js": "^9.18.0",
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-no-catch-all": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^3.0.0",
    "neostandard": "^0.12.2",
    "typescript-eslint": "^8.54.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-security": "^3.0.1",
    "eslint": "^9.39.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
